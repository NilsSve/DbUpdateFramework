45769>>>>>>>>>>>            Set psNumPosRight        to (psRight(Self))
45770>>>>>>>>>>>            Set piNumPosPoints       to (piPoints(Self))
45771>>>>>>>>>>>        End
45771>>>>>>>>>>>>
45771>>>>>>>>>>>        
45771>>>>>>>>>>>        Send ParseFormat sNeg
45772>>>>>>>>>>>        If bCurrency Begin
45774>>>>>>>>>>>            Set pbCurNegThousandsSep to (pbThousandsSep(Self))
45775>>>>>>>>>>>            Set psCurNegLeft         to (psLeft(Self))
45776>>>>>>>>>>>            Set psCurNegRight        to (psRight(Self))
45777>>>>>>>>>>>            Set piCurNegPoints       to (piPoints(Self))
45778>>>>>>>>>>>        End
45778>>>>>>>>>>>>
45778>>>>>>>>>>>        Else Begin
45779>>>>>>>>>>>            Set pbNumNegThousandsSep to (pbThousandsSep(Self))
45780>>>>>>>>>>>            Set psNumNegLeft         to (psLeft(Self))
45781>>>>>>>>>>>            Set psNumNegRight        to (psRight(Self))
45782>>>>>>>>>>>            Set piNumNegPoints       to (piPoints(Self))
45783>>>>>>>>>>>        End
45783>>>>>>>>>>>>
45783>>>>>>>>>>>    End_Procedure
