45683>>>>>>>>>>>        String sLeft sRight sDigit
45683>>>>>>>>>>>        Integer bSep iPos i iDigits
45683>>>>>>>>>>>        
45683>>>>>>>>>>>        // replace any literals. A "/" followed by anything.
45683>>>>>>>>>>>        // some literals are special. $ . , / #
45683>>>>>>>>>>>        Move (Replaces("/$",sFmt,Character(1)))    to sFmt
45684>>>>>>>>>>>        Move (Replaces("/.",sFmt,Character(2)))    to sFmt
45685>>>>>>>>>>>        Move (Replaces("/,",sFmt,Character(3)))    to sFmt
45686>>>>>>>>>>>        Move (Replaces("/"+"/",sFmt,Character(4))) to sFmt
45687>>>>>>>>>>>        Move (Replaces("/#",sFmt,Character(5)))    to sFmt
45688>>>>>>>>>>>        Move (Character(9)) to sDigit
45689>>>>>>>>>>>        Move (Replaces("#",sFmt,sDigit))           to sFmt
45690>>>>>>>>>>>        Move (Replaces("/",sFmt,""))               to sFmt // replace all others
45691>>>>>>>>>>>        
45691>>>>>>>>>>>        Move (Pos(",",sFmt))                 to bSep // if we have any , we use thousand seps
45692>>>>>>>>>>>        If bSep ;            Move (Replaces(",",sFmt,"")) to sFmt // remove all ,
45695>>>>>>>>>>>        
45695>>>>>>>>>>>        Move (Replaces("$",sFmt,psCurrencySymbol(Self))) to sFmt // replace any $ with currency symbol
45696>>>>>>>>>>>        
45696>>>>>>>>>>>        Move (Pos(".",sFmt)) to iPos                 // position of decimal
45697>>>>>>>>>>>        
45697>>>>>>>>>>>        // Move all the special literals back into place before parsing
45697>>>>>>>>>>>        Move (Replaces(Character(1),sFmt,"$")) to sFmt
45698>>>>>>>>>>>        Move (Replaces(Character(2),sFmt,".")) to sFmt
45699>>>>>>>>>>>        Move (Replaces(Character(3),sFmt,",")) to sFmt
45700>>>>>>>>>>>        Move (Replaces(Character(4),sFmt,"/")) to sFmt
45701>>>>>>>>>>>        Move (Replaces(Character(5),sFmt,"#")) to sFmt
45702>>>>>>>>>>>        
45702>>>>>>>>>>>        If (iPos>0) Begin                      // if we have a decimanl point
45704>>>>>>>>>>>            Move 1 to i                        // look for first non # to right and count the #s
45705>>>>>>>>>>>            If (mid(sFmt,1,iPos+i)="*") Begin  // the "*" is special. It means as many as you want
45707>>>>>>>>>>>                Move -2 to iDigits
45708>>>>>>>>>>>                Increment i
45709>>>>>>>>>>>            End
45709>>>>>>>>>>>>
45709>>>>>>>>>>>            While (mid(sFmt,1,iPos+i)=sDigit)
45713>>>>>>>>>>>                Increment i
45714>>>>>>>>>>>            Loop
45715>>>>>>>>>>>>
45715>>>>>>>>>>>            Move (Mid(sFmt,255,iPos+i)) to sRight // everything to the right of the last # is format stuff
45716>>>>>>>>>>>            If (iDigits=0) ;                Move (i-1) to iDigits
45719>>>>>>>>>>>            //
45719>>>>>>>>>>>            Move 1 to i                           // find the first non-# to the left of the point
45720>>>>>>>>>>>            While (mid(sFmt,1,iPos-i)=sDigit)     // everything to the left is format stuff
45724>>>>>>>>>>>                Increment i
45725>>>>>>>>>>>            Loop
45726>>>>>>>>>>>>
45726>>>>>>>>>>>            Move (left(sFmt,iPos-i)) to sLeft
45727>>>>>>>>>>>        End
45727>>>>>>>>>>>>
45727>>>>>>>>>>>        Else Begin                             // we have no decinal
45728>>>>>>>>>>>            Move 0 to iDigits                  // so points is none
45729>>>>>>>>>>>            Move (Pos(sDigit,sFmt)) to iPos    // find first #.
45730>>>>>>>>>>>            If (iPos=0) Begin                  // if none, entire string is left format stuff..wierd!
45732>>>>>>>>>>>                Move sFmt to sLeft
45733>>>>>>>>>>>                Move ""   to sRight
45734>>>>>>>>>>>            End
45734>>>>>>>>>>>>
45734>>>>>>>>>>>            Else Begin
45735>>>>>>>>>>>                Move (left(sFmt,iPos-1)) to sLeft // all char to left of first # is left format stuff
45736>>>>>>>>>>>                Move 1 to i
45737>>>>>>>>>>>                While (mid(sFmt,1,iPos+i)=sDigit) // find last #, all char to right is right format
45741>>>>>>>>>>>                    Increment i
45742>>>>>>>>>>>                Loop
45743>>>>>>>>>>>>
45743>>>>>>>>>>>                Move (Mid(sFmt,255,i+iPos)) to sRight
45744>>>>>>>>>>>            End
45744>>>>>>>>>>>>
45744>>>>>>>>>>>        End
45744>>>>>>>>>>>>
45744>>>>>>>>>>>        // set temporary format properties and exit
45744>>>>>>>>>>>        Set pbThousandsSep to bSep
45745>>>>>>>>>>>        Set psLeft         to sLeft
45746>>>>>>>>>>>        Set psRight        to sRight
45747>>>>>>>>>>>        Set piPoints       to iDigits
45748>>>>>>>>>>>    End_Procedure
