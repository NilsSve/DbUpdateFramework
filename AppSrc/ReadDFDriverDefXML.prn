30372>>>>>>>    // statubar's visibleState to always be false. We correct for this in the statusbar's pbVisible
30372>>>>>>>    Procedure ReleaseComObject
30374>>>>>>>        Handle[] hoObjects hoEmpty
30376>>>>>>>        Handle hoTimer hoObj
