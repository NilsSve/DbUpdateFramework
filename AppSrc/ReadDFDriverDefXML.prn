61335>    Procedure ReadXMLDriverData String sFileName
61338>        DateTime dtExecStart dtExecEnd
61338>        TimeSpan tsTotalTime
61338>        Handle hoXML hoRoot hoList hoBeginAfter hoName hoNextSibling
61338>        Integer iItems i iCount
61338>        Boolean bOK bExists
61338>        String  sNamespace sValue sError
61338>        Variant vRoot vChild vList vBeginAfter vName vNextSibling
61338>        tColumnType[] aColumnType
61338>        tColumnType[] aColumnType
61339>
61339>        Move (CurrentDateTime()) to dtExecStart
61340>        Set paColumnType to aColumnType
61341>
61341>        Get Create (RefClass(cComChilkatXml)) to hoXML
61342>        If (not(IsComObjectCreated(hoXml))) Begin
61344>            Send CreateComObject of hoXml
61345>        End
61345>
61345>        Get ComLoadXmlFile of hoXml sFileName to bOK
61346>        If (bOK <> True) Begin
61348>            Get ComLastErrorText of hoXML to sError
61349>            Send Info_Box ("Sorry, couldn't load the XML file. Error:\n" + sError)
61350>            Procedure_Return
61351>        End
61351>
61351>        Get ComGetSelf of hoXML to vBeginAfter
61352>        If (IsComObject(vBeginAfter)) Begin
61354>            Get Create (RefClass(cComChilkatXml)) to hoBeginAfter
61355>            Set pvComObject of hoBeginAfter to vBeginAfter
61356>        End
61356>
61356>        Get ComSearchForContent of hoXML vBeginAfter "name" "df_field_native_type" to vName
61357>        If (IsComObject(vName)) Begin
61359>            Get Create (RefClass(cComChilkatXml)) to hoName
61360>            Set pvComObject of hoName to vName
61361>        End
61361>        If (IsNullComObject(vName)) Begin
61363>            Send Info_Box 'The tag "df_field_native_type" not found. It is needed to enumerate data types.'
61364>            Procedure_Return
61365>        End
61365>        
61365>        // Get the parent object, from where we can continue searching:
61365>        Get ComGetParent of hoName to vRoot
61366>        If (IsComObject(vRoot)) Begin
61368>            Get Create (RefClass(cComChilkatXml)) to hoRoot
61369>            Set pvComObject of hoRoot to vRoot
61370>        End
61370>        
61370>        // Set baseline at parent level:
61370>        Get ComGetSelf of hoRoot to vBeginAfter
61371>        If (IsComObject(vBeginAfter)) Begin
61373>            Get Create (RefClass(cComChilkatXml)) to hoBeginAfter
61374>            Set pvComObject of hoBeginAfter to vBeginAfter
61375>        End
61375>
61375>        //  Navigate to the enumValues node:
61375>        Get ComSearchForTag of hoRoot vBeginAfter "enumValues" to vList
61376>        If (IsComObject(vList)) Begin
61378>            Get Create (RefClass(cComChilkatXml)) to hoList
61379>            Set pvComObject of hoList to vList
61380>        End
61380>        
61380>        // Set baseline at enum values start object:
61380>        Get ComGetSelf of hoList to vBeginAfter
61381>        If (IsComObject(vBeginAfter)) Begin
61383>            Get Create (RefClass(cComChilkatXml)) to hoBeginAfter
61384>            Set pvComObject of hoBeginAfter to vBeginAfter
61385>        End
61385>        
61385>        // Find the first "enumValue" enumeration:
61385>        Get ComNumChildren of hoList to iItems
61386>        Get ComSearchForTag2 of hoList vBeginAfter "enumValue" to bExists
61387>        If (bExists = False) Begin
61389>            Send Info_Box 'No "enumValue" found (!). Nothing to enumerate.'
61390>            Procedure_Return
61391>        End
61391>
61391>        Decrement iItems
61392>        For iCount from 0 to iItems
61398>            Send CollectDriverAttributes hoList
61399>
61399>            Get ComNextSibling of hoList to vNextSibling
61400>            If (IsComObject(vNextSibling)) Begin
61402>                Get Create (RefClass(cComChilkatXml)) to hoNextSibling
61403>                Set pvComObject of hoNextSibling to vNextSibling
61404>            End
61404>            Send Destroy of hoList
61405>            Move hoNextSibling to hoList
61406>        Loop
61407>
61407>        Send Destroy of hoRoot
61408>        Send Destroy of hoXML
61409>
61409>        Move (CurrentDateTime()) to dtExecEnd
61410>        Move (dtExecEnd - dtExecStart) to tsTotalTime
61411>        Send Info_Box ("Ready! Total Time:" * String(tsTotalTime))
61412>    End_Procedure
